<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.situ.ems_spring.mapper.AuthMapper">

    <select id="selectById" resultType="com.situ.ems_spring.pojo.Auth">
        SELECT * FROM auth WHERE id = #{id};
    </select>
    <select id="selectAll" resultType="com.situ.ems_spring.pojo.Auth">
        select * from auth;
    </select>
</mapper>