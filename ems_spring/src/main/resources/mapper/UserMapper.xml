<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.situ.ems_spring.mapper.UserMapper">

    <select id="login" resultType="com.situ.ems_spring.pojo.Users">
        select * from users where id=#{id} and pwd=#{password}
    </select>
</mapper>